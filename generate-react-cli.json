{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component":"template/generic_function/TemplateName.jsx"
      }
    },
    "layout": {
      "path": "src/App/components/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component":"template/generic_layout/TemplateName.jsx",
        "style":"template/generic_layout/TemplateName.module.css",
        "test":"template/generic_layout/TemplateName.test.jsx"
      }
    }
  }
}